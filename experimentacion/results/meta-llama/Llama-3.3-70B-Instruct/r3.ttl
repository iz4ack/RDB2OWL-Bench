@prefix :      <http://example.org/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Clase Territorio
:Territorio a owl:Class ;
    rdfs:label "Territorio" ;
    rdfs:comment "Representa un territorio" .

# Propiedades de Territorio
:nombre a owl:DatatypeProperty ;
    rdfs:label "nombre" ;
    rdfs:domain :Territorio ;
    rdfs:range xsd:string .

:tipo a owl:DatatypeProperty ;
    rdfs:label "tipo" ;
    rdfs:domain :Territorio ;
    rdfs:range xsd:string .

# Clase ParteDe
:ParteDe a owl:Class ;
    rdfs:label "ParteDe" ;
    rdfs:comment "Representa la relación entre un territorio y su parte" .

# Propiedades de ParteDe
:id_padre a owl:ObjectProperty ;
    rdfs:label "id_padre" ;
    rdfs:domain :ParteDe ;
    rdfs:range :Territorio .

:id_hijo a owl:ObjectProperty ;
    rdfs:label "id_hijo" ;
    rdfs:domain :ParteDe ;
    rdfs:range :Territorio .

# Clase LimitaCon
:LimitaCon a owl:Class ;
    rdfs:label "LimitaCon" ;
    rdfs:comment "Representa la relación entre dos territorios que limitan" .

# Propiedades de LimitaCon
:territorio1 a owl:ObjectProperty ;
    rdfs:label "territorio1" ;
    rdfs:domain :LimitaCon ;
    rdfs:range :Territorio .

:territorio2 a owl:ObjectProperty ;
    rdfs:label "territorio2" ;
    rdfs:domain :LimitaCon ;
    rdfs:range :Territorio .

