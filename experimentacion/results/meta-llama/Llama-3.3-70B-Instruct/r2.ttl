@prefix :      <http://example.org/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Clases
:Departamento a owl:Class .
:Proyecto a owl:Class .
:Empleado a owl:Class .
:EmpleadoTecnico a owl:Class ;
    rdfs:subClassOf :Empleado .
:EmpleadoAdministrativo a owl:Class ;
    rdfs:subClassOf :Empleado .
:EmpleadoJefe a owl:Class ;
    rdfs:subClassOf :Empleado .
:Tarea a owl:Class .
:Asignacion a owl:Class .

# Propiedades de objeto
:tieneJefe a owl:ObjectProperty ;
    rdfs:domain :Departamento ;
    rdfs:range :EmpleadoJefe .
:tieneDepartamento a owl:ObjectProperty ;
    rdfs:domain :Proyecto ;
    rdfs:range :Departamento .
:tieneProyecto a owl:ObjectProperty ;
    rdfs:domain :Tarea ;
    rdfs:range :Proyecto .
:tieneTarea a owl:ObjectProperty ;
    rdfs:domain :Asignacion ;
    rdfs:range :Tarea .
:tieneEmpleado a owl:ObjectProperty ;
    rdfs:domain :Asignacion ;
    rdfs:range :Empleado .
:tieneProyectoAsignado a owl:ObjectProperty ;
    rdfs:domain :Asignacion ;
    rdfs:range :Proyecto .

# Propiedades de datos
:nombre a owl:DatatypeProperty ;
    rdfs:domain :Departamento ;
    rdfs:range xsd:string .
:presupuesto a owl:DatatypeProperty ;
    rdfs:domain :Departamento ;
    rdfs:range xsd:decimal .
:nombreProyecto a owl:DatatypeProperty ;
    rdfs:domain :Proyecto ;
    rdfs:range xsd:string .
:presupuestoProyecto a owl:DatatypeProperty ;
    rdfs:domain :Proyecto ;
    rdfs:range xsd:decimal .
:nombreEmpleado a owl:DatatypeProperty ;
    rdfs:domain :Empleado ;
    rdfs:range xsd:string .
:apellidoEmpleado a owl:DatatypeProperty ;
    rdfs:domain :Empleado ;
    rdfs:range xsd:string .
:especialidad a owl:DatatypeProperty ;
    rdfs:domain :EmpleadoTecnico ;
    rdfs:range xsd:string .
:horasTrabajadas a owl:DatatypeProperty ;
    rdfs:domain :EmpleadoAdministrativo ;
    rdfs:range xsd:integer .
:antiguedadAÃ±os a owl:DatatypeProperty ;
    rdfs:domain :EmpleadoJefe ;
    rdfs:range xsd:integer .
:descripcionTarea a owl:DatatypeProperty ;
    rdfs:domain :Tarea ;
    rdfs:range xsd:string .
:fechaInicio a owl:DatatypeProperty ;
    rdfs:domain :Asignacion ;
    rdfs:range xsd:date .

