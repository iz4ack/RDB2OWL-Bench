@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix uni: <http://example.com/university/> .

uni:Department a owl:Class .
uni:DepartmentID a owl:DatatypeProperty ;
    rdfs:domain uni:Department ;
    rdfs:range xsd:integer .
uni:departmentFullName a owl:DatatypeProperty ;
    rdfs:domain uni:Department ;
    rdfs:range xsd:string .

uni:Student a owl:Class .
uni:StudentID a owl:DatatypeProperty ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:integer .
uni:studentNationalID a owl:DatatypeProperty ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:string .
uni:studentFirstName a owl:DatatypeProperty ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:string .
uni:studentLastName a owl:DatatypeProperty ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:string .
uni:studentEmail a owl:DatatypeProperty ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:string .
uni:studentBirthDate a owl:DatatypeProperty ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:date .
uni:studentEnrollmentDate a owl:DatatypeProperty ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:date .

uni:Professor a owl:Class .
uni:ProfessorID a owl:DatatypeProperty ;
    rdfs:domain uni:Professor ;
    rdfs:range xsd:integer .
uni:professorFirstName a owl:DatatypeProperty ;
    rdfs:domain uni:Professor ;
    rdfs:range xsd:string .
uni:professorLastName a owl:DatatypeProperty ;
    rdfs:domain uni:Professor ;
    rdfs:range xsd:string .
uni:professorEmail a owl:DatatypeProperty ;
    rdfs:domain uni:Professor ;
    rdfs:range xsd:string .
uni:professorBirthDate a owl:DatatypeProperty ;
    rdfs:domain uni:Professor ;
    rdfs:range xsd:date .
uni:professorDepartment a owl:ObjectProperty ;
    rdfs:domain uni:Professor ;
    rdfs:range uni:Department .

uni:Course a owl:Class .
uni:CourseID a owl:DatatypeProperty ;
    rdfs:domain uni:Course ;
    rdfs:range xsd:integer .
uni:courseFullName a owl:DatatypeProperty ;
    rdfs:domain uni:Course ;
    rdfs:range xsd:string .
uni:courseLevel a owl:DatatypeProperty ;
    rdfs:domain uni:Course ;
    rdfs:range xsd:string .
uni:courseCredits a owl:DatatypeProperty ;
    rdfs:domain uni:Course ;
    rdfs:range xsd:integer .
uni:courseDepartment a owl:ObjectProperty ;
    rdfs:domain uni:Course ;
    rdfs:range uni:Department .

uni:Enrollment a owl:Class .
uni:enrolledStudent a owl:ObjectProperty ;
    rdfs:domain uni:Enrollment ;
    rdfs:range uni:Student .
uni:enrolledCourse a owl:ObjectProperty ;
    rdfs:domain uni:Enrollment ;
    rdfs:range uni:Course .
uni:enrollmentDate a owl:DatatypeProperty ;
    rdfs:domain uni:Enrollment ;
    rdfs:range xsd:date .

uni:Evaluation a owl:Class .
uni:evaluatedStudent a owl:ObjectProperty ;
    rdfs:domain uni:Evaluation ;
    rdfs:range uni:Student .
uni:evaluatedCourse a owl:ObjectProperty ;
    rdfs:domain uni:Evaluation ;
    rdfs:range uni:Course .
uni:evaluationGrade a owl:DatatypeProperty ;
    rdfs:domain uni:Evaluation ;
    rdfs:range xsd:decimal .
uni:evaluationDate a owl:DatatypeProperty ;
    rdfs:domain uni:Evaluation ;
    rdfs:range xsd:date .
