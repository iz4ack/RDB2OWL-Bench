@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/university/> .

:Department a owl:Class .
:Student a owl:Class .
:Professor a owl:Class .
:Course a owl:Class .
:Enrollment a owl:Class .
:Evaluation a owl:Class .

:DepartmentID a owl:DatatypeProperty ;
    rdfs:domain :Department ;
    rdfs:range xsd:integer .

:DepartmentName a owl:DatatypeProperty ;
    rdfs:domain :Department ;
    rdfs:range xsd:string .

:DNI a owl:DatatypeProperty ;
    rdfs:domain :Student, :Professor ;
    rdfs:range xsd:string .

:FirstName a owl:DatatypeProperty ;
    rdfs:domain :Student, :Professor ;
    rdfs:range xsd:string .

:LastName a owl:DatatypeProperty ;
    rdfs:domain :Student, :Professor ;
    rdfs:range xsd:string .

:Email a owl:DatatypeProperty ;
    rdfs:domain :Student, :Professor ;
    rdfs:range xsd:string .

:BirthDate a owl:DatatypeProperty ;
    rdfs:domain :Student, :Professor ;
    rdfs:range xsd:date .

:EnrollmentDate a owl:DatatypeProperty ;
    rdfs:domain :Student, :Enrollment ;
    rdfs:range xsd:date .

:CourseID a owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:integer .

:CourseName a owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:string .

:CourseLevel a owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:string .

:Credits a owl:DatatypeProperty ;
    rdfs:domain :Course ;
    rdfs:range xsd:integer .

:Grade a owl:DatatypeProperty ;
    rdfs:domain :Evaluation ;
    rdfs:range xsd:decimal .

:EvaluationDate a owl:DatatypeProperty ;
    rdfs:domain :Evaluation ;
    rdfs:range xsd:date .

:belongsTo a owl:ObjectProperty ;
    rdfs:domain :Professor ;
    rdfs:range :Department .

:teachesIn a owl:ObjectProperty ;
    rdfs:domain :Course ;
    rdfs:range :Department .

:enrolledIn a owl:ObjectProperty ;
    rdfs:domain :Student ;
    rdfs:range :Course .

:evaluatedIn a owl:ObjectProperty ;
    rdfs:domain :Student ;
    rdfs:range :Course .

:hasEnrollment a owl:ObjectProperty ;
    rdfs:domain :Student, :Course ;
    rdfs:range :Enrollment .

:hasEvaluation a owl:ObjectProperty ;
    rdfs:domain :Student, :Course ;
    rdfs:range :Evaluation .

:Student rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty :DNI ;
    owl:cardinality 1 ] .

:Professor rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty :DNI ;
    owl:cardinality 1 ] .

:Course rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty :CourseID ;
    owl:cardinality 1 ] .

:Enrollment rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty :hasEnrollment ;
    owl:cardinality 1 ] .

:Evaluation rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty :hasEvaluation ;
    owl:cardinality 1 ] .

:Department rdfs:subClassOf [ a owl:Restriction ;
    owl:onProperty :DepartmentID ;
    owl:cardinality 1 ] .

:enrolledIn rdfs:subPropertyOf :evaluatedIn .

:evaluatedIn owl:inverseOf :hasEvaluation .

:belongsTo owl:inverseOf :hasProfessor .

:teachesIn owl:inverseOf :hasCourse .

:hasEnrollment owl:inverseOf :enrolledIn .