@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.com/ontology/> .

:Department a owl:Class .
:Project a owl:Class .
:Employee a owl:Class .
:TechnicalEmployee a owl:Class ;
    rdfs:subClassOf :Employee .
:AdministrativeEmployee a owl:Class ;
    rdfs:subClassOf :Employee .
:Manager a owl:Class ;
    rdfs:subClassOf :Employee .
:Task a owl:Class .
:Assignment a owl:Class .

:departmentID a owl:DatatypeProperty ;
    rdfs:domain :Department ;
    rdfs:range xsd:integer .
:fullName a owl:DatatypeProperty ;
    rdfs:domain :Department ;
    rdfs:range xsd:string .
:budget a owl:DatatypeProperty ;
    rdfs:domain :Department ;
    rdfs:range xsd:decimal .
:managerID a owl:DatatypeProperty ;
    rdfs:domain :Department ;
    rdfs:range xsd:integer .

:projectID a owl:DatatypeProperty ;
    rdfs:domain :Project ;
    rdfs:range xsd:integer .
:projectFullName a owl:DatatypeProperty ;
    rdfs:domain :Project ;
    rdfs:range xsd:string .
:projectBudget a owl:DatatypeProperty ;
    rdfs:domain :Project ;
    rdfs:range xsd:decimal .
:departmentIDProject a owl:DatatypeProperty ;
    rdfs:domain :Project ;
    rdfs:range xsd:integer .

:employeeID a owl:DatatypeProperty ;
    rdfs:domain :Employee ;
    rdfs:range xsd:integer .
:firstName a owl:DatatypeProperty ;
    rdfs:domain :Employee ;
    rdfs:range xsd:string .
:lastName a owl:DatatypeProperty ;
    rdfs:domain :Employee ;
    rdfs:range xsd:string .
:departmentIDEmployee a owl:DatatypeProperty ;
    rdfs:domain :Employee ;
    rdfs:range xsd:integer .

:specialty a owl:DatatypeProperty ;
    rdfs:domain :TechnicalEmployee ;
    rdfs:range xsd:string .

:hoursWorked a owl:DatatypeProperty ;
    rdfs:domain :AdministrativeEmployee ;
    rdfs:range xsd:integer .

:yearsOfService a owl:DatatypeProperty ;
    rdfs:domain :Manager ;
    rdfs:range xsd:integer .

:taskID a owl:DatatypeProperty ;
    rdfs:domain :Task ;
    rdfs:range xsd:integer .
:description a owl:DatatypeProperty ;
    rdfs:domain :Task ;
    rdfs:range xsd:string .

:assignmentEmployeeID a owl:DatatypeProperty ;
    rdfs:domain :Assignment ;
    rdfs:range xsd:integer .
:assignmentProjectID a owl:DatatypeProperty ;
    rdfs:domain :Assignment ;
    rdfs:range xsd:integer .
:assignmentTaskID a owl:DatatypeProperty ;
    rdfs:domain :Assignment ;
    rdfs:range xsd:integer .
:startDate a owl:DatatypeProperty ;
    rdfs:domain :Assignment ;
    rdfs:range xsd:date .

:hasManager a owl:ObjectProperty ;
    rdfs:domain :Department ;
    rdfs:range :Manager .
:hasProject a owl:ObjectProperty ;
    rdfs:domain :Department ;
    rdfs:range :Project .
:hasEmployee a owl:ObjectProperty ;
    rdfs:domain :Department ;
    rdfs:range :Employee .
:hasTask a owl:ObjectProperty ;
    rdfs:domain :Project ;
    rdfs:range :Task .
:hasAssignment a owl:ObjectProperty ;
    rdfs:domain :Project ;
    rdfs:range :Assignment .
:assignedTo a owl:ObjectProperty ;
    rdfs:domain :Employee ;
    rdfs:range :Assignment .
:worksOn a owl:ObjectProperty ;
    rdfs:domain :Employee ;
    rdfs:range :Task .
