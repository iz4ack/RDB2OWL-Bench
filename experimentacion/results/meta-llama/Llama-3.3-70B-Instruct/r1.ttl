@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix :      <http://example.org/ontology#> .

# Clases
:Departamento a owl:Class .
:Estudiante a owl:Class .
:Profesor a owl:Class .
:Curso a owl:Class .
:Inscripcion a owl:Class .
:Evaluacion a owl:Class .

# Propiedades de objeto
:tieneDepartamento a owl:ObjectProperty ;
    rdfs:domain :Profesor ;
    rdfs:range :Departamento .
:tieneDepartamento a owl:ObjectProperty ;
    rdfs:domain :Curso ;
    rdfs:range :Departamento .
:estaInscritoEn a owl:ObjectProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range :Curso .
:tieneEvaluacion a owl:ObjectProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range :Evaluacion .

# Propiedades de datos
:ID_Departamento a owl:DatatypeProperty ;
    rdfs:domain :Departamento ;
    rdfs:range xsd:integer .
:Nombre a owl:DatatypeProperty ;
    rdfs:domain :Departamento ;
    rdfs:range xsd:string .
:ID_Estudiante a owl:DatatypeProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:integer .
:DNI a owl:DatatypeProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:string .
:Nombre a owl:DatatypeProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:string .
:Apellido a owl:DatatypeProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:string .
:Email a owl:DatatypeProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:string .
:FechaNacimiento a owl:DatatypeProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:date .
:FechaMatricula a owl:DatatypeProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:date .
:ID_Profesor a owl:DatatypeProperty ;
    rdfs:domain :Profesor ;
    rdfs:range xsd:integer .
:Nombre a owl:DatatypeProperty ;
    rdfs:domain :Profesor ;
    rdfs:range xsd:string .
:Apellido a owl:DatatypeProperty ;
    rdfs:domain :Profesor ;
    rdfs:range xsd:string .
:Email a owl:DatatypeProperty ;
    rdfs:domain :Profesor ;
    rdfs:range xsd:string .
:FechaNacimiento a owl:DatatypeProperty ;
    rdfs:domain :Profesor ;
    rdfs:range xsd:date .
:ID_Curso a owl:DatatypeProperty ;
    rdfs:domain :Curso ;
    rdfs:range xsd:integer .
:Nombre a owl:DatatypeProperty ;
    rdfs:domain :Curso ;
    rdfs:range xsd:string .
:Nivel a owl:DatatypeProperty ;
    rdfs:domain :Curso ;
    rdfs:range xsd:string .
:Creditos a owl:DatatypeProperty ;
    rdfs:domain :Curso ;
    rdfs:range xsd:integer .
:FechaInscripcion a owl:DatatypeProperty ;
    rdfs:domain :Inscripcion ;
    rdfs:range xsd:date .
:Nota a owl:DatatypeProperty ;
    rdfs:domain :Evaluacion ;
    rdfs:range xsd:decimal .
:FechaEvaluacion a owl:DatatypeProperty ;
    rdfs:domain :Evaluacion ;
    rdfs:range xsd:date .
