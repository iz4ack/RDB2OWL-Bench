@prefix : <http://example.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Person a owl:Class .
:NIF a owl:DatatypeProperty ;
     rdfs:domain :Person ;
     rdfs:range xsd:integer .
:email a owl:DatatypeProperty ;
       rdfs:domain :Person ;
       rdfs:range xsd:string .
:first_name a owl:DatatypeProperty ;
            rdfs:domain :Person ;
            rdfs:range xsd:string .

:Employee a owl:Class .
:id a owl:DatatypeProperty ;
    rdfs:domain :Employee ;
    rdfs:range xsd:integer .
:salary a owl:DatatypeProperty ;
        rdfs:domain :Employee ;
        rdfs:range xsd:decimal .

:Consultant a owl:Class .
:hourly_rate a owl:DatatypeProperty ;
             rdfs:domain :Consultant ;
             rdfs:range xsd:decimal .

:Mentors a owl:ObjectProperty ;
         rdfs:domain :Person ;
         rdfs:range :Person .

:Company a owl:Class .
:NIF_Company a owl:DatatypeProperty ;
             rdfs:domain :Company ;
             rdfs:range xsd:string .
:company_name a owl:DatatypeProperty ;
              rdfs:domain :Company ;
              rdfs:range xsd:string .

:Location a owl:Class .
:id_Location a owl:DatatypeProperty ;
             rdfs:domain :Location ;
             rdfs:range xsd:integer .
:street a owl:DatatypeProperty ;
        rdfs:domain :Location ;
        rdfs:range xsd:string .
:city a owl:DatatypeProperty ;
      rdfs:domain :Location ;
      rdfs:range xsd:string .
:province a owl:DatatypeProperty ;
          rdfs:domain :Location ;
          rdfs:range xsd:string .
:coordinates a owl:DatatypeProperty ;
             rdfs:domain :Location ;
             rdfs:range xsd:string .

:WorksWith a owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range :Company .

:HasLocation a owl:ObjectProperty ;
             rdfs:domain :Company ;
             rdfs:range :Location .
