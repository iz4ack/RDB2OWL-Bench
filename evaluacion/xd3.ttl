@prefix : <http://example.org/universidad#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.org/universidad> a owl:Ontology .

# Clases
:Equipo a owl:Class .
:Jugador a owl:Class .
:Entrenador a owl:Class .

# Properties de Jugador
:dniJugador a owl:DatatypeProperty ;
    rdfs:domain :Jugador ;
    rdfs:range xsd:string .

:nombreJugador a owl:DatatypeProperty ;
    rdfs:domain :Jugador ;
    rdfs:range xsd:string .

:apellidoJugador a owl:DatatypeProperty ;
    rdfs:domain :Jugador ;
    rdfs:range xsd:string .

:emailJugador a owl:DatatypeProperty ;
    rdfs:domain :Jugador ;
    rdfs:range xsd:string .

:fechaNacimientoJugador a owl:DatatypeProperty ;
    rdfs:domain :Jugador ;
    rdfs:range xsd:date .

:fechaMatricula a owl:DatatypeProperty ;
    rdfs:domain :Jugador ;
    rdfs:range xsd:date .

# Properties de Entrenador
:nombreEntrenador a owl:DatatypeProperty ;
    rdfs:domain :Entrenador ;
    rdfs:range xsd:string .

:apellidoEntrenador a owl:DatatypeProperty ;
    rdfs:domain :Entrenador ;
    rdfs:range xsd:string .

:emailEntrenador a owl:DatatypeProperty ;
    rdfs:domain :Entrenador ;
    rdfs:range xsd:string .

:fechaNacimientoEntrenador a owl:DatatypeProperty ;
    rdfs:domain :Entrenador ;
    rdfs:range xsd:date .

:dniEntrenador a owl:DatatypeProperty ;
    rdfs:domain :Entrenador ;
    rdfs:range xsd:string .

# ObjectProperty: Entrenador -> Equipo
:perteneceAEquipo a owl:ObjectProperty ;
    rdfs:domain :Entrenador ;
    rdfs:range :Equipo .

