@prefix :      <http://example.org/ontology#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .


:UniversidadOntology a owl:Ontology ;
    rdfs:label "Ontología de Universidad (ejemplo)" ;
    rdfs:comment "Recurso 1. Relacion 1..N, relacion N..N, clase normal, relacion N..N con atributos intermedios, " .

##### Departamento #####
:Departamento a owl:Class ;
    rdfs:label "Departamento" ;
    rdfs:comment "Representa un departamento dentro de la universidad." ;
    owl:hasKey :hasIdDepartamento ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasIdDepartamento ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .
## DatatypeProperties
:hasIdDepartamento a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Departamento ;
    rdfs:range xsd:integer ;
    rdfs:label "ID_Departamento" ;
    rdfs:comment "Código del departamento (ej. '01')." .

:hasNombreDepartamento a owl:DatatypeProperty ;
    rdfs:domain :Departamento ;
    rdfs:range xsd:string ;
    rdfs:label "nombreDepartamento" ;
    rdfs:comment "Nombre del departamento (ej. 'Matemáticas')." .

##### Persona #####
:Persona a owl:Class ;
    rdfs:label "Persona" ;
    rdfs:comment "Representa a una persona." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasNombre ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasApellido ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .

:hasNombre a owl:DatatypeProperty ;
    rdfs:domain :Persona ;
    rdfs:range xsd:string ;
    rdfs:label "nombrePersona" ;
    rdfs:comment "Nombre de la persona." .

:hasApellido a owl:DatatypeProperty ;
    rdfs:domain :Persona ;
    rdfs:range xsd:string ;
    rdfs:label "apellidoPersona" ;
    rdfs:comment "Apellido de la persona." .

:hasEmail a owl:DatatypeProperty ;
    rdfs:domain :Persona ;
    rdfs:range xsd:string ;
    rdfs:label "emailPersona" ;
    rdfs:comment "Correo electrónico de la persona." .

:hasFechaNacimiento a owl:DatatypeProperty ;
    rdfs:domain :Persona ;
    rdfs:range xsd:date ;
    rdfs:label "fechaNacimiento" ;
    rdfs:comment "Fecha de nacimiento de la persona." .

##### Estudiante #####
:Estudiante a owl:Class ;
    rdfs:label "Estudiante" ;
    rdfs:comment "Representa a un estudiante de la universidad." ;
    owl:hasKey :hasIdEstudiante ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasIdEstudiante ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf :Persona .

## DatatypeProperties
:hasIdEstudiante a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:integer ;
    rdfs:label "ID_Estudiante" ;
    rdfs:comment "Código del estudiante." .
:hasFechaMatriculaEstudiante a owl:DatatypeProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range xsd:date ;
    rdfs:label "fechaMatriculaEstudiante" ;
    rdfs:comment "Fecha de matrícula del estudiante." .
## ObjectProperties
:hasProfesor a owl:ObjectProperty ;
    rdfs:domain :Estudiante ;
    rdfs:range :Profesor ;
    rdfs:label "profesorTutor" ;
    rdfs:comment "Profesor perteneciente a un departamento asociado a un curso del estudiante." .


##### Profesor #####
:Profesor a owl:Class ;
    rdfs:label "Profesor" ;
    rdfs:comment "Representa a un profesor de la universidad." ;
    owl:hasKey :hasIdProfesor ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasIdProfesor ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf :Persona .
## DatatypeProperties
:hasIdProfesor a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Profesor ;
    rdfs:range xsd:integer ;
    rdfs:label "ID_Profesor" ;
    rdfs:comment "Código del profesor." .

## ObjectProperties
:pertenceDepartamentoProfesor a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Profesor ;
    rdfs:range :Departamento ;
    rdfs:label "ID_Departamento" ;
    rdfs:comment "Departamento al que pertenece el profesor." .


##### Curso #####
:Curso a owl:Class ;
    rdfs:label "Curso" ;
    rdfs:comment "Representa un curso ofrecido por la universidad." ;
    owl:hasKey :hasIdCurso ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasIdCurso ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .
## DatatypeProperties
:hasIdCurso a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Curso ;
    rdfs:range xsd:integer ;
    rdfs:label "ID_Curso" ;
    rdfs:comment "Código del curso." .

:hasNombreCurso a owl:DatatypeProperty ;
    rdfs:domain :Curso ;
    rdfs:range xsd:string ;
    rdfs:label "nombreCurso" ;
    rdfs:comment "Nombre o título del curso." .

:hasNivelCurso a owl:DatatypeProperty ;
    rdfs:domain :Curso ;
    rdfs:range xsd:string ;
    rdfs:label "nivelCurso" ;
    rdfs:comment "Nivel del curso (ej. 'Básico', 'Avanzado')." .

:hasCreditosCurso a owl:DatatypeProperty ;
    rdfs:domain :Curso ;
    rdfs:range xsd:integer ;
    rdfs:label "creditosCurso" ;
    rdfs:comment "Cantidad de créditos del curso." .

## ObjectProperties
:perteneceDepartamentoCurso a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Curso ;
    rdfs:range :Departamento ;
    rdfs:label "ID_Departamento" ;
    rdfs:comment "Departamento al que pertenece el curso." .

##### Inscripcion #####
:Inscripcion a owl:Class ;
    rdfs:label "Inscripción" ;
    rdfs:comment "Relaciona a un estudiante con un curso (matrícula)." ;
    owl:hasKey (:hasCursoInscripcion :hasEstudianteInscripcion);
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasEstudianteInscripcion ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasCursoInscripcion ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .
## DatatypeProperties
:hasFechaInscripcion a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Inscripcion ;
    rdfs:range xsd:date ;
    rdfs:label "fechaInscripcion" ;
    rdfs:comment "Fecha de inscripción del estudiante en el curso." .

## ObjectProperties
:hasEstudianteInscripcion a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Inscripcion ;
    rdfs:range :Estudiante ;
    rdfs:label "estudiante Inscrito" ;
    rdfs:comment "Estudiante inscrito en el curso." .

:hasCursoInscripcion a owl:ObjectProperty,owl:FunctionalProperty ;
    rdfs:domain :Inscripcion ;
    rdfs:range :Curso ;
    rdfs:label "curso Inscrito" ;
    rdfs:comment "Curso en el que está inscrito el estudiante." .

##### Evaluacion #####
:Evaluacion a owl:Class ;
    rdfs:label "Evaluación" ;
    rdfs:comment "Registra la calificación de un estudiante en un curso." ;
    owl:hasKey :hasIdEvaluacion ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasIdEvaluacion ;
        owl:cardinality "1"^^xsd:nonNegativeInteger
    ] .
## DatatypeProperties
:hasNotaEvaluacion a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Evaluacion ;
    rdfs:range xsd:decimal ;
    rdfs:label "notaEvaluacion" ;
    rdfs:comment "Nota obtenida por el estudiante en el curso." .

:hasFechaEvaluacion a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Evaluacion ;
    rdfs:range xsd:date ;
    rdfs:label "fechaEvaluacion" ;
    rdfs:comment "Fecha de la evaluación." .

:hasIdEvaluacion a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:domain :Evaluacion ;
    rdfs:range xsd:integer ;
    rdfs:label "ID_evaluacion" ;
    rdfs:comment "Identificador de la evaluación." .
## ObjectProperties
:hasEstudianteEvaluado a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Evaluacion ;
    rdfs:range :Estudiante ;
    rdfs:label "estudiante Evaluado" ;
    rdfs:comment "Estudiante evaluado en la evaluación." .  

:hasCursoEvaluado a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:domain :Evaluacion ;
    rdfs:range :Curso ;
    rdfs:label "curso Evaluado" ;
    rdfs:comment "Curso evaluado en la evaluación." .